<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cs639.aslproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Letters"
            android:label="@string/title_activity_letters"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Words"
            android:label="@string/title_activity_words"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Numbers"
            android:label="@string/title_activity_numbers"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>